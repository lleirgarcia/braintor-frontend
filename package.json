{
    "name": "ytoptimizer",
    "version": "1.0.0",
    "scripts": {
      "install:all": "npm install && cd transcriptor-backend && npm install && cd ../transcriptor-frontend && npm install",
      "build:backend": "cd transcriptor-backend && npm run build",
      "build:frontend": "cd transcriptor-frontend && npm run build",
      "build": "npm-run-all --parallel build:backend build:frontend",
      "start": "npm-run-all --parallel start:backend start:frontend",
      "start:backend": "cd transcriptor-backend && npm run dev",
      "start:frontend": "cd transcriptor-frontend && npm run start"
    },
    "devDependencies": {
      "npm-run-all": "^4.1.5"
    }
  }
  